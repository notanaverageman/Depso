; This file is for unifying the coding style for different editors and IDEs.
; More information at http://EditorConfig.org

root = true

[*]
end_of_line = CRLF

[*.svg]
end_of_line = LF

[*.cs]
indent_style = tab
indent_size = 4

[*.csproj]
indent_style = space
indent_size = 2

[*.props]
indent_style = space
indent_size = 2

dotnet_diagnostic.IDE0055.severity = None
dotnet_diagnostic.IDE0058.severity = None